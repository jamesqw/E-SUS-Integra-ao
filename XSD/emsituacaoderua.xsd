<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://esus.ufsc.br/emsituacaoderua" version="1.0">

  <xs:complexType name="emSituacaoDeRuaTransport">
