      <xs:element maxOccurs="1" minOccurs="0" name="grauParentescoFamiliarFrequentado" type="xs:string"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="higienePessoalSituacaoRua" type="xs:long"/>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="origemAlimentoSituacaoRua" type="xs:long"/>
